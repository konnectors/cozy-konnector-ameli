{
  "name": "cozy-konnector-ameli",
  "version": "1.0.6",
  "description": "Cozy Konnector for Ameli",
  "repository": {
    "type": "git",
    "url": "https://github.com/konnectors/cozy-konnector-ameli.git"
  },
  "keywords": [],
  "author": "",
  "license": "AGPL-3.0",
  "main": "src/index.js",
  "renovate": {
    "extends": [
      "config:base"
    ],
    "patch": {
      "automerge": true
    }
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "jest": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "scripts": {
    "start": "node src/index.js",
    "test": "konitor testit .",
    "clean": "rm -f *_ameli.pdf",
    "dev": "cozy-konnector-dev src/index.js",
    "standalone": "npm run clean && cozy-konnector-standalone src/index.js",
    "standalone:record": "REPLAY=record cozy-konnector-standalone src/index.js",
    "standalone:replay": "REPLAY=replay cozy-konnector-standalone src/index.js",
    "build": "webpack",
    "format": "prettier --write '**/*.{js,json}'",
    "precommit": "eslint .",
    "lint": "eslint .",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo=${DEPLOY_REPOSITORY:-https://$GITHUB_TOKEN@github.com/konnectors/cozy-konnector-ameli.git}"
  },
  "dependencies": {
    "cozy-konnector-libs": "3.5.3"
  },
  "devDependencies": {
    "copy-webpack-plugin": "4.5.1",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "git-directory-deploy": "1.5.1",
    "husky": "0.14.3",
    "konitor": "0.5.3",
    "prettier": "1.11.1",
    "pretty-quick": "1.4.1",
    "webpack": "4.4.1",
    "webpack-cli": "2.0.14"
  }
}
